#include <cstdlib>
#include <locale.h>
#include <iostream>
using namespace std;


int main()
{
setlocale(LC_ALL, "");

    int a;
    
    cout << "cuantos dias tiene el año actual: " << endl;
    
    cin >> a;
    
    if (a==365)
    {
    
    cout << "El año no es biciesto" << endl;
    }
    else
    {
        cout << "El año es biciesto. ";
    }
    
    return 0;
}
