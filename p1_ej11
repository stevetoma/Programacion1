#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    int x;
    double a;
    
    cout << "ingrese el numero a ser evaluado" << endl;
    
    cin >> x;
    
    if (x%2==0)
    {
        a = pow(x,2);
        cout << a << " Es par" << endl;
    }    
    else{
         a = pow(x,3);
        cout << a << " Es impar" << endl;
    }
    return 0;
}
