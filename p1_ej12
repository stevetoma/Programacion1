#include <iostream>
using namespace std;

int main() {
    char x;
    cout << "Ingrese la letra: ";
    cin >> x;

    x = tolower(x);

    if (x == 'a' || x == 'e' || x == 'i' || x == 'o' || x == 'u') {
        cout << x << " es una vocal" << endl;
    } else {
        cout << x << " es una consonante" << endl;  
    }

    return 0;
}
