#include <cstdlib>
#include <locale.h>
#include <iostream>
using namespace std;


int main()
{
setlocale(LC_ALL, "");

    int a;
    
    cout << "Ingrese la edad: " << endl;
    
    cin >> a;
    
    if (a<=6)
    {
    
    cout << "Infancia" << endl;
    }
    
    else if (a>=7 && a<=12)
    {
        cout << "NiÃ±ez" <<endl;
    }
    
    else if (a>=13 && a <=20)
    {
        cout << "Adolescencia"<< endl; 
    }
    
    else if (a>=21 && a <= 25)
    {
        cout << "juventud"<<endl;
    }
    
    else if (a>=26 && a <=60)
    {
        cout << "Adultez"<<endl;
    }
    
    else if (a>60)
    {
        cout << "Ancianidad"<<endl;
    }
    
    return 0;
}
